{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "barryvdh/laravel-debugbar": "2.4.3",
        "chumper/zipper": "1.0.2",
        "doctrine/dbal": "2.8.0",
        "dompdf/dompdf": "0.8.2",
        "encore/laravel-admin": "1.5.17",
        "fideloper/proxy": "3.3.4",
        "guzzlehttp/guzzle": "6.3.3",
        "laravel-admin-ext/scheduling": "1.0.1",
        "laravel/framework": "5.5.44",
        "laravel/tinker": "1.0.8",
        "maatwebsite/excel": "3.0.10",
        "mike42/escpos-php": "2.0.2",
        "symfony/css-selector": "3.4.4"
    },
    "require-dev": {
        "filp/whoops": "2.3.1",
        "fzaninotto/faker": "1.8.0",
        "mockery/mockery": "1.2.0",
        "phpunit/phpunit": "6.5.13",
        "symfony/thanks": "1.1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
